          }
        }
      }
    },
    "@libs/components": {
      "projectType": "library",
      "root": "projects/libs/components",
      "sourceRoot": "projects/libs/components/src",
      "prefix": "libs",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
