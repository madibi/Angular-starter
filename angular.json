          }
        }
      }
    },
    "@libs/widgets": {
      "projectType": "library",
      "root": "projects/libs/widgets",
      "sourceRoot": "projects/libs/widgets/src",
      "prefix": "libs",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/libs/widgets/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/libs/widgets/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/libs/widgets/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/libs/widgets/src/test.ts",
            "tsConfig": "projects/libs/widgets/tsconfig.spec.json",
