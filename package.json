{
  "name": "libs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "new_feature": "ng g ng-samurai:generate-subentry new_feature --project @libs/components",
    "build:core:watch": "ng build --project=@libs/core --watch",
    "build:layouts:watch": "ng build --project=@libs/layouts --watch",
    "build:components:watch": "ng build --project=@libs/components --watch",
    "build:scss": "scss-bundle --config scss-bundle.config.json",
